<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{{ 'titre'|trans }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="eticketing Louvre - an OpenClassRooms project" />
    <meta name="keywords" content="project, symfony3, eticketing, Louvre, OpenClassRooms" />
    <meta name="author" content="Natacha NOEL" />
    
    
    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" type="image/x-icon">
    <link rel="icon" href="{{ asset('favicon.ico') }}" type="image/x-icon">
    
    <link href="https://fonts.googleapis.com/css?family=Lato:300,400,700" rel="stylesheet">
    
    <!-- Animate.css -->
    <link rel="stylesheet" href="{{ asset('assets/css/animate.css') }}">
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href="{{ asset('assets/css/icomoon.css') }}">
    <!-- Themify Icons-->
    <link rel="stylesheet" href="{{ asset('assets/css/themify-icons.css') }}">
    <!-- Bootstrap  -->
    <link rel="stylesheet" href="{{ asset('assets/css/bootstrap.css') }}">
    
    <!-- Background -->
    <link rel="stylesheet" href="{{ asset('assets/css/bg.css') }}">
    
    <!-- Magnific Popup -->
    <link rel="stylesheet" href="{{ asset('assets/css/bootstrap-datepicker.min.css') }}">
    
    <!-- Owl Carousel  -->
    <link rel="stylesheet" href="{{ asset('assets/css/owl.carousel.min.css') }}">
    <link rel="stylesheet" href="{{ asset('assets/css/owl.theme.default.min.css') }}">
    
    <!-- Theme style  -->
    <link rel="stylesheet" href="{{ asset('assets/css/style.css') }}">
    
    <!-- Modernizr JS -->
    <script src="{{ asset('assets/js/modernizr-2.6.2.min.js') }}"></script>
    <!-- FOR IE9 below -->
    <!--[if lt IE 9]>
    <script src="{{ asset('assets/js/respond.min.js') }}"></script>
    <![endif]-->

</head>
<body>
    
    <div class="gtco-loader"></div>
    
     <div id="page" class="content">
        
        
        <!-- <div class="page-inner"> -->
        
        <div id="gtco-header" class="gtco-cover gtco-cover-md content" role="banner">
            <div class="overlay"></div>
            <div class="gtco-container">
                <div class="row">
                    <div class="col-md-12 col-md-offset-0 text-left">
                        <div class="row row-mt-15em">
                            <div class="col-md-6 mt-text animate-box" data-animate-effect="fadeInUp">
                                <div id="gtco-logo">
                                    
                                    <a href="{{ path('index') }}">
                                        <div class="img">
                                            
                                            <img src="{{ asset('assets/images/louvre-760.jpg') }}" srcset="{{ asset('assets/images/louvre-320.jpg') }} 320w,{{ asset('assets/images/louvre-640.jpg') }} 640vw" sizes="35vw" alt="Logo du Louvre">
                                        </div>
                                    </a>
                                </div>
                                <h1>{{ 'sous_titre'|trans }}</h1>
                            </div>
                                {% block body %}
                                {% endblock %}
                            <div class="row footer">
                                <div class="col-md-12">
                                    Copyright Louvre 2017 - <a href="{{ path('tarifs') }}"> {{ 'tarifs'|trans }} </a>  - <a href="{{ path('mentions') }}"> {{ 'mentions'|trans }} </a> - CGV - FAQ
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    
    
    
                            <!-- jQuery -->
    
         <script src="{{ asset('assets/js/jquery.min.js') }}"></script>
    <!-- jQuery Easing -->
    <script src="{{ asset('assets/js/jquery.easing.1.3.js') }}"></script>
    <!-- Bootstrap -->
    <script src="{{ asset('assets/js/bootstrap.min.js') }}"></script>
    <!-- Waypoints -->
    <script src="{{ asset('assets/js/jquery.waypoints.min.js') }}"></script>
    <!-- Carousel -->
    <script src="{{ asset('assets/js/owl.carousel.min.js') }}"></script>
    <!-- countTo -->
    <script src="{{ asset('assets/js/jquery.countTo.js') }}"></script>
    <!-- slider -->
    <script src="{{ asset('assets/js/bg.js') }}"></script>

    <!-- Stellar Parallax -->
    <script src="{{ asset('assets/js/jquery.stellar.min.js') }}"></script>

    <!-- Magnific Popup -->
    <script src="{{ asset('assets/js/jquery.magnific-popup.min.js') }}"></script>
    <script src="{{ asset('assets/js/magnific-popup-options.js') }}"></script>

    <!-- Datepicker -->
    <script src="{{ asset('assets/js/bootstrap-datepicker.min.js') }}"></script>
    <script type="text/javascript">
        $(document).ready(function() {
            var closeDays = ['14-07-2017'];
            {% set MyArray = ['01-11-2017','11-11-2017','25-12-2017','01-01-2018','01-05-2018','08-05-2018'] %}
            {% for number in MyArray %}
                closeDays.push('{{ number }}');
            {% endfor %}
            
            $('.js-datepicker').datepicker({
                startDate: new Date(),
                format: 'dd-mm-yyyy',
                daysOfWeekDisabled:[2,0],
                datesDisabled: closeDays,
                weekStart: 1
            });

            $('.js2-datepicker').datepicker({
                startDate: new Date(),
                format: 'dd-mm-yyyy',
                weekStart: 1
            });
        })
    
    </script>
    <!-- Main -->
    <script src="{{ asset('assets/js/main.js') }}"></script>
         
    <script src="{{ asset('assets/js/nbBillets.js') }}"></script>
    <script src="{{ asset('assets/js/supprBillets.js') }}"></script>




     </div>
</body>
</html>
